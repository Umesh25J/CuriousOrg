<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duplicate_Account</fullName>
    <active>true</active>
    <errorConditionFormula>Duplicate_Check__c  == True</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Cannot Have Duplicate Account With Same Name</errorMessage>
</ValidationRule>
